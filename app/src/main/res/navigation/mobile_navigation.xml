<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.project.ui.ProgramsFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_programs" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.project.ui.InspirationFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_inspiration" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.project.ui.NewProgramFragment"
        android:label="@string/title_notifications"
